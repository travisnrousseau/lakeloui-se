+/**
+ * Grid Definition Template 3.1 - Rotated Latitude/Longitude
+ * Same as 3.0 (octets 15-72) plus: LaD (73-76), LoV (77-80), angle (81-84).
+ * [Read more...](https://www.nco.ncep.noaa.gov/pmb/docs/grib2/grib2_doc/grib2_temp3-1.shtml)
+ */
+const template31 = (section: Buffer) => {
+  const base = template30(section)
+  const basicAngle = section.readUInt32BE(38)
+  const subdivisions = section.readUInt32BE(42)
+  const ratio = basicAngle === 0 ? 1e-6 : basicAngle / subdivisions
+  const LaD = section.readInt32BE(72) * ratio
+  const LoV = section.readInt32BE(76) * ratio
+  return { ...base, LaD, LoV, gridUnits: 'degrees' as const }
+}
+
+const lookupTemplate31 = (templateValues: ReturnType<typeof template31>) => {
+  return {
+    ...templateValues,
+    shape: lookupTable32(templateValues.shape)
+  }
+}
